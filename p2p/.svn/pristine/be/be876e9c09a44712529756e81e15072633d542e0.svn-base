.main {
  width:1001px;
  margin:auto;
  position:relative;
}
body {
  color:#4c4c44;
}
/*login*/
.login {
  height:40px;
  background:#383838;
}
.login p {
  color:#c2c4c3;
  font-size:14px;
  line-height:40px;
  float:left;
}
.login p span {
  color:#fff;
  font-size:18px;
}
.login .help {
  float:right;
  font-size:14px;
}
.login .help ul,.login .help ol {
  float:left;
}
.login .help ul li {
  float:left;
  line-height:40px;
  background:url(../image/yyx.jpg) no-repeat right center;
  padding-right:12px;
  margin-right:10px;
}
.login .help ol li {
  float:left;
  line-height:40px;
  margin-right:13px;
}
.login .help a {
  color:#c2c4c3;
}
.login .help a:hover {
  text-decoration:underline;
}
/*头部*/
.header {
  background:url(../image/header.jpg) repeat-x;
  height:91px;
  border-top:1px #f2f2f2 solid;
}
/*logo*/
.logo {
  float:left;
  margin-right:55px;
}
.logo img{
  margin-top:13px;
  float:left;
  margin-right:38px;
}
.logo dl {
  float:left;
  display:inline-block;
  text-align:center;
  color:#383838;
  margin-top:25px;
}
.logo dl dt {
  margin-bottom:10px;
  font-size:14px;
}
.logo dl dd {
  position:relative;
  cursor:pointer;
  z-index:999;
}
.logo dl dd .city {
   width:200px;
   height:100px;
   border:1px #ccc solid;
   background:#fff;
   position:absolute;
   top:14px;
   left:0px;
   display:none;
}
.logo dl dd .city h4{
  height:30px;
  line-height:30px;
  font-size:14px;
  background: #F5F5F5;
  color: #656565;
  text-align:left;
  padding-left:10px;
}
.logo dl dd .city ul {
  width:180px;
  margin:auto;
}
.logo dl dd .city ul li {
  line-height:35px;
  margin-right:5px;
  border-bottom:1px #dfdfdf dashed;
  text-align:left;
}
.logo dl dd .city ul li a{
  color: #2f52e9;
  cursor: pointer;
  font: 15px "微软雅黑", "Arial", "Helvetica", "sans-serif";
  margin-right:10px;
}
/*导航*/
.menu {
  float:left;
  font-size:18px;
}
.menu ul li {
  float:left;
  background:url(../image/dh-yyx.jpg) no-repeat right center;
  padding-right:19px;
  margin-right:16px;
  line-height:91px;
}
.menu ul li.hover a {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
.menu ul li a:hover {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}

/*子导航*/
.sub_menu {
  width:100%;
  height:30px;
  background:url(../image/header.jpg) repeat-x;
  border-bottom:1px #ccc solid;
  position:fixed;
  top:0px;
  font-size:14px;
  z-index:999;
  display:none;
}
.sub_menu ul {
  margin-left:10px;
}
.sub_menu ul li {
  float:left;
  line-height:30px;
  margin-right:30px;
}
/*类型*/
.leixing {
  margin-top:21px;
}
.leixing h2 {
  background:url(../image/dizhi2.png) no-repeat left center;
  padding-left:20px;
  height:20px;
  line-height:20px;
  font-size:16px;
}
.leixing h2 a span {
  color:#018bce;
}
.leixing .info{
  background:#fff;
  border:1px #e0e0e0 solid;
  margin-top:15px;
  overflow:hidden;
}
.leixing .info_l {
  float:left;
  width:646px;
  border-right:1px #e0e0e0 solid;
  padding:0px 22px;
}
.leixing .info_l dl {
  border-bottom:1px #e0e0e0 dashed;
  overflow:hidden;
  padding-top:17px;
  padding-bottom:11px;
}
.leixing .info_l dl dt {
  float:left;
  margin-top:7px;
}
.leixing .info_l dl dd {
  float:left;
  width:550px;
}
.leixing .info_l dl dd a {
  display:inline-block;
  padding:7px 10px 5px 10px;
  margin:0 15px;
}
.leixing .info_l dl dd a.hover {
  background:#1194d2;
  border-radius:3px;
  color:#fff;
}
.leixing .info_l dl dd a:hover {
  background:#1194d2;
  border-radius:3px;
  color:#fff;
}
.leixing .info_r{
  width:308px;
  float:left;
  text-align:center;
  color:#bfbfbf;
  font-size:28px;
  font-weight:bold;
  height:auto;
  line-height:600%;
}
.leixing .info_r p{
  
}
.leixing .info_r p span {
  color:#008bce;
  font-size:30px;
}
/*内容*/
.content {
  margin-top:10px;
}
/*内容左*/
.content_l {
  width:690px;
  float:left;
  margin-right:7px;
  background:#fff;
  border:1px #e3e3e3 solid;
}
.content_l .liebiao {
  height:130px;
  border-bottom:1px #e3e3e3 solid;
  position:relative;
  padding-left:10px;
  padding-top:2px;
}
.content_l .liebiao.hover{
  background:#d6edf9;
  cursor:pointer;
}
.content_l .liebiao dl{

}
.content_l .liebiao dl dt{
  float:left;
  margin-right:20px;
  margin-top:13px;
}
.content_l .liebiao dl dd{
  float:left;
  width:489px;
  margin-top:10px;
}
.content_l .liebiao dl dd h4 {
  font-size:16px;
  color:#7f7f7f;
  font-weight:bold;
}
.content_l .liebiao dl dd ul {
  margin-top:24px;
}
.content_l .liebiao dl dd ul li {
  float:left;
  margin-right:30px;
  +margin-right:30px;
}
.content_l .liebiao dl dd ul li span {
  color:#e7420a;
}
.content_l .liebiao dl dd ol {
  margin-top:20px;
}
.content_l .liebiao dl dd ol li {
  float:left;
  margin-right:37px;
  line-height:15px;
}
.content_l .liebiao dl dd ol .jindu{
  width:188px;
  height:18px;
  background:#dde4ea;
}
.content_l .liebiao dl dd ol .jindu b{
  width:188px;
  height:18px;
  display:block;
  background:#3ba9e6;
  width:10%;
}
.content_l .liebiao dl dd ol li span {
  color:#469bcb;
}
.content_l .liebiao dl dd p {
  width:111px;
  height:35px;
  background:#ff6600;
  position:absolute;
  top:1px;
  right:9px;
  text-align:center;
  line-height:35px;
  font-size:14px;
  font-weight:bold;
}
.content_l .liebiao dl dd p a {
  color:#fff;
}
.content_l .liebiao dl dd p a span {
  display:block;
  background:#06cf48;
}
.content_l .liebiao em img {
  position:absolute;
  top:0px;
  left:0px;
}
.content_l .page {
  width:303px;
  margin:auto;
  padding-top:34px;
  padding-bottom:25px;
  overflow:hidden;
}
.content_l .page ul li{
  float:left;
  border:1px #e0e0e0 solid;
  margin-right:8px;
}
.content_l .page ul li a{
  display:block;
  padding:8px;
}
.content_l .page ul li a:hover{
  background:#2980b9;
  color:#fff;
}
.content_l .page ul li a:hover span {
  border-right-color:#fff;
}
.content_l .page ul li a:hover em {
  border-left-color:#fff;
}
.content_l .page ul li span {
  display:inline-block;
  width:0px;
  height:0px;
  border:5px transparent solid;
  border-left:none;
  border-right-color:#999999;
}
.content_l .page ul li em {
  display:inline-block;
  width:0px;
  height:0px;
  border:5px transparent solid;
  border-right:none;
  border-left-color:#999999;
}

/*内容右*/
.content_r {
  float:left;
  width:301px;
}
.content_r .info {
  border:1px #dfdfdf solid;
  padding-bottom:10px;
  margin-top:9px;
  background:#fff;
}
.content_r .info h2,.content_r .info h3{
  height:38px;
  background:url(../image/border.jpg) no-repeat bottom;
  font-size:16px;
}
.content_r .info h2 span,.content_r .info h3 span{
  line-height:38px;
  padding-left:28px;
  margin-left:12px;
  float:left;
}
.content_r .info h2 span {
  background:url(../image/jieshao2.jpg) no-repeat left center;;
}
.content_r .info h3 span {
  background:url(../image/hezuo2.jpg) no-repeat left center;;
}
.content_r .info h2 p{
  float:right;
  line-height:43px;
  margin-right:25px;
  position:relative;
  font-size:12px;
}
.content_r .info h2 p a{
  color:#999999;
}
.content_r .info h2 p em,.content_r .info h2 p i{
  display:block;
  width:0px;
  height:0px;
  border:5px transparent solid;
  border-right:none;
  position:relative;
}
.content_r .info h2 p em{
  border-left-color:#999999;
  position:absolute;
  top:16px;
  right:-11px;
}
.content_r .info h2 p i{
  border-left-color:#fff;
  position:absolute;
  top:16px;
  right:-9px;
}
.content_r .info ul {
  padding-left:24px;
  margin-top:15px;
}
.content_r .info ul li {
  background:url(../image/dian.jpg) no-repeat left center;
  padding-left:15px;
  height:27px;
  line-height:27px;
}
.content_r .info .com {
  border-top:1px #dfdfdf solid;
  margin-top:4px;
}
.content_r .info img {
  width:292px;
  display:block;
  margin:auto;
  padding-top:10px; 
}


/*底部*/
.footer {
  height:260px;
  background:#2c2c2c;
  margin-top:50px;
  padding-top:30px;
  font-size:14px;
}
.footer .info {
  width:715px;
  margin:auto;
  overflow:hidden;
  border-top:1px #6f6d6e solid;
}
.footer .info ul {
   margin-top:23px;
}
.footer .info ul li {
  color:#969698;
  text-align:center;
}
.footer .info ul li a{
  color:#969698;
  display:inline-block;
  margin-right:20px;
}
.footer .link {
  width:493px;
  overflow:hidden;
  margin:auto;
}
.link_l{
  width:283px;
  float:left;
}
.link_l ul {
  float:left;
  margin-top:20px;
}
.link_l ul li {
  color:#969698;
  line-height:50px;
}
.link_l ul li span {
  background:url(../image/tel.jpg) no-repeat left center;
  padding-left:18px;
}
.link_l ul li em {
  background:url(../image/zhong.jpg) no-repeat left center;
  padding-left:18px;
}.link_l ul li img {
  margin-right:10px;
  margin-left:105px;
}
.link_r {
  width:165px;
  float:right;
  padding-top:17px;
}
.link_r dl {
  width:68px;
  text-align:center;
  float:left;
  margin-left:14px;
}
.link_r dl dt {
  margin-bottom:10px;
}
.renzheng {
  text-align:center;
}
.renzheng p {
  line-height:90px;
}
.renzheng p a {
  margin-right:20px;
}
/*返回顶部*/
.back{
  position:fixed;
  right:20px;
  bottom:50px;
  z-index:999;
}
.back ul li {

}
.back ul .xinxi {
  height:38px;
  padding:3px;
}
.back ul .xinxi p {
  width:38px;
  height:38px;
  background:url(../image/xinxi.png) no-repeat;
  float:right;
  cursor:pointer;
}
.back ul .xinxi p.hover {
  background:url(../image/xinxi-hover.png) no-repeat;
}

.back ul .back_btn {
  width:38px;
  height:38px;
  background:url(../image/back.png) no-repeat;
  float:right;
  margin-right:4px;
  margin-top:10px;
  display:none;
  cursor:pointer;
}
.back ul .back_btn.hover {
  background:url(../image/back-hover.png) no-repeat;
}

