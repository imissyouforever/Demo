.main {
  width:1001px;
  margin:auto;
  position:relative;
}
body {
  color:#4c4c44;
}
/*login*/
.login {
  height:40px;
  background:#383838;
}
.login p {
  color:#c2c4c3;
  font-size:14px;
  line-height:40px;
  float:left;
}
.login p span {
  color:#fff;
  font-size:18px;
}
.login .help {
  float:right;
  font-size:14px;
}
.login .help ul,.login .help ol {
  float:left;
}
.login .help ul li {
  float:left;
  line-height:40px;
  background:url(../image/yyx.jpg) no-repeat right center;
  padding-right:12px;
  margin-right:10px;
}
.login .help ol li {
  float:left;
  line-height:40px;
  margin-right:13px;
}
.login .help a {
  color:#c2c4c3;
}
.login .help a:hover {
  text-decoration:underline;
}
/*头部*/
.header {
  background:url(../image/header.jpg) repeat-x;
  height:91px;
  border-top:1px #f2f2f2 solid;
}
/*logo*/
.logo {
  float:left;
  margin-right:55px;
}
.logo img{
  margin-top:13px;
  float:left;
  margin-right:38px;
}
.logo dl {
  float:left;
  display:inline-block;
  text-align:center;
  color:#383838;
  margin-top:25px;
}
.logo dl dt {
  margin-bottom:10px;
  font-size:14px;
}
.logo dl dd {
  position:relative;
  cursor:pointer;
  z-index:999;
}
.logo dl dd .city {
   width:200px;
   height:100px;
   border:1px #ccc solid;
   background:#fff;
   position:absolute;
   top:14px;
   left:0px;
   display:none;
}
.logo dl dd .city h4{
  height:30px;
  line-height:30px;
  font-size:14px;
  background: #F5F5F5;
  color: #656565;
  text-align:left;
  padding-left:10px;
}
.logo dl dd .city ul {
  width:180px;
  margin:auto;
}
.logo dl dd .city ul li {
  line-height:35px;
  margin-right:5px;
  border-bottom:1px #dfdfdf dashed;
  text-align:left;
}
.logo dl dd .city ul li a{
  color: #2f52e9;
  cursor: pointer;
  font: 15px "微软雅黑", "Arial", "Helvetica", "sans-serif";
  margin-right:10px;
}
/*导航*/
.menu {
  float:left;
  font-size:18px;
}
.menu ul li {
  float:left;
  background:url(../image/dh-yyx.jpg) no-repeat right center;
  padding-right:19px;
  margin-right:16px;
  line-height:91px;
}
.menu ul li.hover a {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
.menu ul li a:hover {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
/*子导航*/
.sub_menu {
  width:100%;
  height:30px;
  background:url(../image/header.jpg) repeat-x;
  border-bottom:1px #ccc solid;
  position:fixed;
  top:0px;
  font-size:14px;
  z-index:999;
  display:none;
}
.sub_menu ul {
  margin-left:10px;
}
.sub_menu ul li {
  float:left;
  line-height:30px;
  margin-right:30px;
}
/*banner*/
.banner_dl {
  +overflow:hidden;
}
.banner{
  height:100%;
  position:relative;
}
.banner ul li {
  display:none;
}
.banner ul li img {
  width:100%;
}
.banner .left {
  width:40px;
  height:62px;
  position:absolute;
  margin-top:-31px;
  top:50%;
  left:102px;
  cursor:pointer;
  background:url(../image/left-btn.png) no-repeat;
}
.banner .left.hover {
  background:url(../image/left-btn-hover.png) no-repeat;
}
.banner .right {
  width:40px;
  height:62px;
  position:absolute;
  margin-top:-31px;
  top:50%;
  right:102px;
  cursor:pointer;
  background:url(../image/right-btn.png) no-repeat;
}
.banner .right.hover {
  background:url(../image/right-btn-hover.png) no-repeat;
}
.banner ol {
  position:absolute;
  bottom:22px;
  left:50%;
  margin-left:-65px;
}
.banner ol li {
  height:16px;
  width:16px;
  background:url(../image/yuandian.png) no-repeat;
  float:left;
  margin-right:15px;
  cursor:pointer;
}
.banner ol li.hover {
  background:url(../image/yuandian-hover.png) no-repeat;
}
/*登陆*/
.dl {
  width:330px;
  height:300px;
  position:absolute;
  right:10px;
  top:-340px;
  z-index:995;
}
.dl .info {
  width:330px;
  height:300px;
  background:rgba(0,0,0,.2);
  filter:alpha(opacity=20);
  background:#000\9;
  border-radius:10px;
  position:absolute;
}
.dl form {
  width:297px;
  margin:auto;
  margin-top:21px;
  position:relative;
}
.dl input {
  color:#cccccc;
}
.dl .text {
  width:233px;
  height:47px;
  background:url(../image/yonghu.png) no-repeat;
  border:none;
  padding-left:64px;
  line-height:47px\9;
}
.dl .password {
  width:233px;
  height:47px;
  background:url(../image/password.png) no-repeat;
  border:none;
  padding-left:64px;
  margin-top:10px;
  margin-bottom:10px;
  line-height:47px\9;
}
.dl label{
  color:#fff;
  margin-left:100px;
  margin-right:30px;
}
.dl span a {
  color:#fff;
}
.dl .btn {
  width:296px;
  height:46px;
  background:url(../image/btn.png) no-repeat;
  cursor:pointer;
  border:none;
  margin-top:10px;
}
.dl em a {
  display:block;
  width:296px;
  height:46px;
  background:url(../image/zhuce.png) no-repeat;
  margin-top:15px;
}
/*什么是债交所*/
