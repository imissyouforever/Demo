.main {
  width:1001px;
  margin:auto;
  position:relative;
}
body {
  color:#4c4c44;
}
/*login*/
.login {
  height:40px;
  background:#383838;
}
.login p {
  color:#c2c4c3;
  font-size:14px;
  line-height:40px;
  float:left;
}
.login p span {
  color:#fff;
  font-size:18px;
}
.login .help {
  float:right;
  font-size:14px;
}
.login .help ul,.login .help ol {
  float:left;
}
.login .help ul li {
  float:left;
  line-height:40px;
  background:url(../image/yyx.jpg) no-repeat right center;
  padding-right:12px;
  margin-right:10px;
}
.login .help ol li {
  float:left;
  line-height:40px;
  margin-right:13px;
}
.login .help a {
  color:#c2c4c3;
}
.login .help a:hover {
  text-decoration:underline;
}
/*头部*/
.header {
  background:url(../image/header.jpg) repeat-x;
  height:91px;
  border-top:1px #f2f2f2 solid;
}
/*logo*/
.logo {
  float:left;
  margin-right:55px;
}
.logo img{
  margin-top:13px;
  float:left;
  margin-right:38px;
}
.logo dl {
  float:left;
  display:inline-block;
  text-align:center;
  color:#383838;
  margin-top:25px;
}
.logo dl dt {
  margin-bottom:10px;
  font-size:14px;
}
.logo dl dd {
  position:relative;
  cursor:pointer;
  z-index:999;
}
.logo dl dd .city {
   width:200px;
   height:100px;
   border:1px #ccc solid;
   background:#fff;
   position:absolute;
   top:14px;
   left:0px;
   display:none;
}
.logo dl dd .city ul {
  width:180px;
  margin:10px auto;
}
.logo dl dd .city ul li {
  float:left;
  margin-right:5px;
}
/*导航*/
.menu {
  float:left;
  font-size:18px;
}
.menu ul li {
  float:left;
  background:url(../image/dh-yyx.jpg) no-repeat right center;
  padding-right:19px;
  margin-right:16px;
  line-height:91px;
}
.menu ul li.hover a {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
.menu ul li a:hover {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
/*子导航*/
.sub_menu {
  width:100%;
  height:30px;
  background:url(../image/header.jpg) repeat-x;
  border-bottom:1px #ccc solid;
  position:fixed;
  top:0px;
  font-size:14px;
  z-index:999;
  display:none;
}
.sub_menu ul {
  margin-left:10px;
}
.sub_menu ul li {
  float:left;
  line-height:30px;
  margin-right:30px;
}
/*主体*/
.content{
  margin-top:38px;
}
.content_l {
  width:180px;
  margin-right:22px;
  float:left;
}
.content_l .gywm {
  background:#f2f2f2;
  border:1px #b8bbbe solid;
  border-radius:4px;
}
.content_l .gywm h2 {
  height:45px;
  line-height:45px;
  text-align:center;
  color:#8c8c8c;
  font-size:19px;
  
}
.content_l .gywm ul li{
  height:44px;	
  line-height:44px;
  text-align:center;
  font-size:15px;
  border-top:1px #c2c6c8 solid;
  background:url(../image/images/yh_03.jpg) repeat-x;
  cursor:pointer;
  
}
.content_l .gywm ul li a {
   margin-left:29px;
   margin-right:14px;
   color:#9f9e9e;
}
	
.content_l .gywm ul li b {
   display:inline-block;
   background:url(../image/images/yh1.png) no-repeat;
   height:29px; width:29px;
   vertical-align:middle;
   
	
}
.content_l .gywm ul li:hover {
   color:#fffefe;	
   background:url(../image/images/yh2_03.jpg) repeat-x;height:44px;
}
.content_l .gywm ul li:hover a {
   color:#FFF;
}
.content_l .gywm ul li:hover b {
   margin-right:2px;
   background:url(../image/images/yh3.png) no-repeat;
   height:29px; width:29px;
   
}

/*主体右*/
.content_r {
  width:777px;
  float:left;
  margin-top:5px;
}
.content_r .right {
   border:1px #c2c5c7 solid;
   border-radius:4px;
}
.content_r .right h3 {
   height:39px;
   line-height:39px;
   font-size:18px;
   text-indent:22px;
   border-bottom:1px #c2c5c7 solid;
}
.content_r .right ul {
	height:37px;
	line-height:37px;
	background:#e8e9ea;
	
	
}
.content_r .right ul li {
	float:left;
	color:#333;
	margin-left:68px;
	font-size:16px;
}
.content_r .right dl{
	width:691px;
	height:160px;
	border:1px #c2c5c7 solid;
	border-radius:5px;
	margin:13px auto;
	background:#f1f2f1;
}
.content_r .right dl dt ol {
	height:40px;
	line-height:40px; 
	font-family:"宋体";
	border-bottom:1px #c2c5c7 solid;
}
.content_r .right dl dt ol li {
	float:left;
	width:66px;
	font-size:16px;
	text-align:center;
	
}
.content_r .right dl dt ol .r1 {
	margin-left:7px;
}
.content_r .right dl dt ol .r2 {
	margin-left:52px;
}
.content_r .right dl dt ol .r3 {
	margin-left:56px;
}
.content_r .right dl dt ol .r4 {
	margin-left:50px;
}
.content_r .right dl dt ol .r5 {
	margin-left:50px;
}
.content_r .right dl dt ol .r6 {
	margin-left:46px;
}
.content_r .right dl dd {
	width:220px;
	height:87px;
    background:url(../image/images/yh4_03.jpg) no-repeat right center;	
	font-family:"宋体";
	float:left;
	text-align:center;
	margin-top:18px;
}
.content_r .right dl .yb {
	background:none;
}

.content_r .right dl dd p {
	font-size:14px;
}
.content_r .right dl dd .d1 {
	margin:26px 0px 19px 0px;
	color:#ff6600;
	font-weight:bold
}
.content_r .right .jl {
	height:26px;
	font-family:"宋体";
	width:777px;
	text-align:center;
	border-bottom:1px #c2c5c7 solid;
}
.content_r .right .jl span {
	margin-right:28px;
}
.content_r .right .xy {
	width:328px;
	height:30px;
	margin:10px 31%;
	
}
.content_r .right .xy li {
	height:22px;
	float:left;
	border:1px #c2c5c7 solid;
	line-height:22px;
	padding:4px 10px;
	margin-right:13px;
}
.content_r .right .xy li:hover a{
	color:#000;
}
.content_r .right .xy .xy1:hover {
	background:#2980b9;	
}
.content_r .right .xy .xy1:hover a {
	color:#FFF;
}

/*底部*/
.footer {
  height:260px;
  background:#2c2c2c;
  margin-top:138px;
  padding-top:30px;
  font-size:14px;
}
.footer .info {
  width:715px;
  margin:auto;
  overflow:hidden;
  border-top:1px #6f6d6e solid;
}
.footer .info ul {
   margin-top:23px;
}
.footer .info ul li {
  color:#969698;
  text-align:center;
}
.footer .info ul li a{
  color:#969698;
  display:inline-block;
  margin-right:20px;
}
.footer .link {
  width:493px;
  overflow:hidden;
  margin:auto;
}
.link_l{
  width:283px;
  float:left;
}
.link_l ul {
  float:left;
  margin-top:20px;
}
.link_l ul li {
  color:#969698;
  line-height:50px;
}
.link_l ul li span {
  background:url(../image/tel.jpg) no-repeat left center;
  padding-left:18px;
}
.link_l ul li em {
  background:url(../image/zhong.jpg) no-repeat left center;
  padding-left:18px;
}.link_l ul li img {
  margin-right:10px;
  margin-left:105px;
}
.link_r {
  width:165px;
  float:right;
  padding-top:17px;
}
.link_r dl {
  width:68px;
  text-align:center;
  float:left;
  margin-left:14px;
}
.link_r dl dt {
  margin-bottom:10px;
}
.renzheng {
  text-align:center;
}
.renzheng p {
  line-height:90px;
}
.renzheng p a {
  margin-right:20px;
}
/*返回顶部*/
.back{
  position:fixed;
  right:20px;
  bottom:50px;
  z-index:999;
}
.back ul li {

}
.back ul .xinxi {
  height:38px;
  padding:3px;
}
.back ul .xinxi.hover {
  border:1px solid #ccc;
  background:#f9f9f9;
}
.back ul .xinxi p {
  width:38px;
  height:38px;
  background:url(../image/xinxi.png) no-repeat;
  float:right;
  cursor:pointer;
}
.back ul .xinxi p.hover {
  background:url(../image/xinxi-hover.png) no-repeat;
}
.back ul .xinxi span {
  float:left;
  line-height:38px;
  margin-right:5px;
  display:none;
}

.back ul .back_btn {
  width:38px;
  height:38px;
  background:url(../image/back.png) no-repeat;
  float:right;
  margin-right:4px;
  margin-top:10px;
  display:none;
  cursor:pointer;
}
.back ul .back_btn.hover {
  background:url(../image/back-hover.png) no-repeat;
}





























