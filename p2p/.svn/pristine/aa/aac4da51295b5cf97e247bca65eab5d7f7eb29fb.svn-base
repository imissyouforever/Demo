/*主体右*/
.content_r {
  width:742px;
  float:left;
  margin-top:5px;
}
.content_r .title h2 {
  font-size:19px;
  width:583px;
  text-align:center;
  color:#2ea4d2;
  line-height:19px;
  margin:auto;
  background:url(../image/title.jpg) no-repeat;
}
.content_r .title img {
  margin-top:32px;
}
/*乾多多*/
.content_r .duoduo {
  margin-top:26px;
}
.content_r .duoduo h2 {
  color:#1c3a54;
  height:25px;
  line-height:25px;
  border-bottom:2px #7388be solid;
  font-size:16px;
  font-weight:bold;
  margin-bottom:20px;
}
.content_r .duoduo h2 span {
  background:url(../image/qianduouo.jpg) no-repeat left center;
  padding-left:33px;
}
.content_r .duoduo p img {
  float:left;
  margin-right:38px;
}
.content_r .duoduo p {
  text-indent:2em;
  line-height:2;
}


/*底部*/
.footer {
  height:260px;
  background:#2c2c2c;
  margin-top:60px;
  padding-top:30px;
  font-size:14px;
}
.footer .info {
  width:715px;
  margin:auto;
  overflow:hidden;
  border-top:1px #6f6d6e solid;
}
.footer .info ul {
   margin-top:23px;
}
.footer .info ul li {
  color:#969698;
  text-align:center;
}
.footer .info ul li a{
  color:#969698;
  display:inline-block;
  margin-right:20px;
}
.footer .link {
  width:493px;
  overflow:hidden;
  margin:auto;
}
.link_l{
  width:283px;
  float:left;
}
.link_l ul {
  float:left;
  margin-top:20px;
}
.link_l ul li {
  color:#969698;
  line-height:50px;
}
.link_l ul li span {
  background:url(../image/tel.jpg) no-repeat left center;
  padding-left:18px;
}
.link_l ul li em {
  background:url(../image/zhong.jpg) no-repeat left center;
  padding-left:18px;
}.link_l ul li img {
  margin-right:10px;
  margin-left:105px;
}
.link_r {
  width:165px;
  float:right;
  padding-top:17px;
}
.link_r dl {
  width:68px;
  text-align:center;
  float:left;
  margin-left:14px;
}
.link_r dl dt {
  margin-bottom:10px;
}
.renzheng {
  text-align:center;
}
.renzheng p {
  line-height:90px;
}
.renzheng p a {
  margin-right:20px;
}
/*返回顶部*/
.back{
  position:fixed;
  right:20px;
  bottom:50px;
  z-index:999;
}
.back ul li {

}
.back ul .xinxi {
  height:38px;
  padding:3px;
}
.back ul .xinxi p {
  width:38px;
  height:38px;
  background:url(../image/xinxi.png) no-repeat;
  float:right;
  cursor:pointer;
}
.back ul .xinxi p.hover {
  background:url(../image/xinxi-hover.png) no-repeat;
}

.back ul .back_btn {
  width:38px;
  height:38px;
  background:url(../image/back.png) no-repeat;
  float:right;
  margin-right:4px;
  margin-top:10px;
  display:none;
  cursor:pointer;
}
.back ul .back_btn.hover {
  background:url(../image/back-hover.png) no-repeat;
}
.link_r dd a { cursor:pointer}
