.main {
  width:1001px;
  margin:auto;
  position:relative;
}
body {
  color:#4c4c44;
}
/*login*/
.login {
  height:40px;
  background:#383838;
}
.login p {
  color:#c2c4c3;
  font-size:14px;
  line-height:40px;
  float:left;
}
.login p span {
  color:#fff;
  font-size:18px;
}
.login .help {
  float:right;
  font-size:14px;
}
.login .help ul,.login .help ol {
  float:left;
}
.login .help ul li {
  float:left;
  line-height:40px;
  background:url(../image/yyx.jpg) no-repeat right center;
  padding-right:12px;
  margin-right:10px;
}
.login .help ol li {
  float:left;
  line-height:40px;
  margin-right:13px;
}
.login .help a {
  color:#c2c4c3;
}
.login .help a:hover {
  text-decoration:underline;
}
/*头部*/
.header {
  background:url(../image/header.jpg) repeat-x;
  height:91px;
  border-top:1px #f2f2f2 solid;
}
/*logo*/
.logo {
  float:left;
  margin-right:55px;
}
.logo img{
  margin-top:13px;
  float:left;
  margin-right:38px;
}
.logo dl {
  float:left;
  display:inline-block;
  text-align:center;
  color:#383838;
  margin-top:25px;
}
.logo dl dt {
  margin-bottom:10px;
  font-size:14px;
}
.logo dl dd {
  position:relative;
  cursor:pointer;
  z-index:999;
}
.logo dl dd .city {
   width:200px;
   height:100px;
   border:1px #ccc solid;
   background:#fff;
   position:absolute;
   top:14px;
   left:0px;
   display:none;
}
.logo dl dd .city h4{
  height:30px;
  line-height:30px;
  font-size:14px;
  background: #F5F5F5;
  color: #656565;
  text-align:left;
  padding-left:10px;
}
.logo dl dd .city ul {
  width:180px;
  margin:auto;
}
.logo dl dd .city ul li {
  line-height:35px;
  margin-right:5px;
  border-bottom:1px #dfdfdf dashed;
  text-align:left;
}
.logo dl dd .city ul li a{
  color: #2f52e9;
  cursor: pointer;
  font: 15px "微软雅黑", "Arial", "Helvetica", "sans-serif";
  margin-right:10px;
}
/*导航*/
.menu {
  float:left;
  font-size:18px;
}
.menu ul li {
  float:left;
  background:url(../image/dh-yyx.jpg) no-repeat right center;
  padding-right:19px;
  margin-right:16px;
  line-height:91px;
}
.menu ul li.hover a {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}
.menu ul li a:hover {
  background:url(../image/hover.jpg) no-repeat bottom center;
  padding-bottom:34px;
}/*子导航*/
.sub_menu {
  width:100%;
  height:30px;
  background:url(../image/header.jpg) repeat-x;
  border-bottom:1px #ccc solid;
  position:fixed;
  top:0px;
  font-size:14px;
  z-index:999;
  display:none;
}
.sub_menu ul {
  margin-left:10px;
}
.sub_menu ul li {
  float:left;
  line-height:30px;
  margin-right:30px;
}
/*主体*/
.content{
  margin-top:38px;
}
/*主体左*/
